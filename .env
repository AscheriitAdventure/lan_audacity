# Nom de l'application
APP_NAME="Lan Audacity"
APP_AUTHOR="Geoffrey TRONCHE"
APP_VERSION=1.0.0
APP_ORGANIZATION="Student Software Development 2024"
APP_DESCRIPTION="Application de gestion de LAN/WAN"
APP_CREATION_DATE='2023-01-16 9:00:00'
APP_UPDATE_DATE=2025-03-26
APP_COPYRIGHT="Geoffrey TRONCHE"

GIT_REPOSITORY_URL="https://github.com/AscheriitAdventure/lan_audacity"

# Environnement de l'application
STATUS_ENV=prod
APP_TYPE=desktop
APP_DEBUG=true
LOG_LEVEL=warning
APP_CONF_LOG=data/conf/logs.yaml
