DEVICETYPE_FILE_RSC=data/json/type_object.json

# Outils utilisés
MIB_LIBRARY=assets/snmp-mibs/mibs/
MIBS_PY=assets/snmp-mibs/mibs-py/

# Git Repo Support
GIT_REPO_SUPPORT=["https://github.com/ecceman/affinity.git", "https://github.com/trevoro/snmp-mibs.git"]
